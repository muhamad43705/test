#body-login
  %h2.title-form Sign Up
  = form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f|
    = devise_error_messages!
    .field
      = f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Enter Email"
    .field
      = f.password_field :password, autocomplete: "off", placeholder: "Enter Password"
    .field
      = f.password_field :password_confirmation, autocomplete: "off", placeholder: "Enter Password Confirmation"
    .actions
      = f.submit "Sign up"
  = render "devise/shared/links"
